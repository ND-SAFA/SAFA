{
  "steps": [
    {
      "jobs": [
        {
          "object_type": "CREATE_DATASETS",
          "job_args": {
            "random_seed": 68,
            "output_dir": "[OUTPUT_PATH]/create_dataset/cm1"
          },
          "trainer_dataset_manager": {
            "train_dataset_creator": {
              "object_type": "TRACE",
              "project_reader": {
                "object_type": "STRUCTURE",
                "project_path": "[DATA_PATH]/cm1"
              },
              "filter_unlinked_artifacts": true
            },
            "val_dataset_creator": {
              "object_type": "SPLIT",
              "split_strategy": "SPLIT_BY_LINK",
              "val_percentage": 0.25
            },
            "eval_dataset_creator": {
              "object_type": "SPLIT",
              "split_strategy": "SPLIT_BY_LINK",
              "val_percentage": 0.25
            },
            "augmenter": {
              "steps": [
                {
                  "object_type": "RESAMPLE"
                }
              ]
            }
          }
        }
      ]
    }
  ],
  "output_dir": "[OUTPUT_PATH]/create_dataset/cm1"
}
