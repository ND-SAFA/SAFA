{
  "artifacts": {
    "Requirements": {
      "parser": "XML",
      "path": "source.xml",
      "cols": "CCHIT-artifacts",
      "params": {
        "xpath": "/artifacts/artifact"
      }
    },
    "Regulatory Codes": {
      "parser": "XML",
      "path": "target.xml",
      "cols": "CCHIT-artifacts",
      "params": {
        "xpath": "/artifacts/artifact"
      }
    }
  },
  "traces": {
    "requirements2regulatorycodes.csv": {
      "source": "Requirements",
      "target": "Regulatory Codes",
      "path": "answer.txt",
      "parser": "CSV"
    }
  },
  "conversions": {
    "CCHIT-artifacts": {
      "art_id": "id",
      "art_title": "content"
    }
  },
  "overrides": {
    "ALLOW_MISSING_TARGET": true,
    "ALLOW_MISSING_SOURCE": true
  }
}