{
  "steps": [
    {
      "jobs": [
        {
          "job_args": {
            "random_seed": 42
          },
          "object_type": "CREATE_DATASETS",
          "trainer_dataset_manager": {
            "train_dataset_creator": {
              "object_type": "TRACE",
              "project_reader": {
                "object_type": "STRUCTURE",
                "project_path": "~/desktop/safa/datasets/safa/source"
              }
            },
            "val_dataset_creator": {
              "object_type": "SPLIT",
              "split_strategy": "SPLIT_BY_SOURCE",
              "val_percentage": 0.75
            },
            "eval_dataset_creator": {
              "object_type": "SPLIT",
              "split_strategy": "SPLIT_BY_SOURCE",
              "val_percentage": 0.10
            },
            "augmenter": {
              "steps": [
                {
                  "object_type": "RESAMPLE"
                }
              ]
            }
          },
          "export_path": "~/desktop/safa/datasets/safa/exported"
        }
      ]
    }
  ]
}
