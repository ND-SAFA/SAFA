{
  "steps": [
    {
      "jobs": [
        {
          "object_type": "MULTI_LAYER_HGEN",
          "starting_hgen_job": {
            "hgen_args": {
              "export_dir": "[OUTPUT_PATH]/hgen/[CURRENT_PROJECT]",
              "source_layer_id": "[SOURCE_LAYER_ID_LIST]",
              "target_type": "[TARGET_TYPE_1]",
              "create_project_summary": "[CREATE_PROJECT_SUMMARY_BOOL]",
              "generate_trace_links": true,
              "interactive_mode": true,
              "dataset_creator": {
                "object_type": "PROMPT",
                "trace_dataset_creator": {
                  "project_reader": {
                    "object_type": "STRUCTURE",
                    "project_path": "[DATA_PATH]/[CURRENT_PROJECT]"
                  }
                }
              }
            }
          },
          "target_types": "[TARGET_TYPES_LIST]"
        }
      ]
    }
  ]
}