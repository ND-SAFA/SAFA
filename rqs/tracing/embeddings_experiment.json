{
  "steps": [
    {
      "jobs": [
        {
          "object_type": "RANKING",
          "ranking_pipeline": "EMBEDDING",
          "export_dir": "[OUTPUT_PATH]/embedding/[CURRENT_PROJECT]",
          "generate_explanations": "[GENERATE_EXPLANATIONS_OPTIONAL]",
          "embedding_model_name": {
            "*": [
              "sentence-transformers/all-roberta-large-v1", "sentence-transformers/all-MiniLM-L6-v2",
              "BAAI/bge-m3"
            ]
          },
          "dataset_creator": {
            "object_type": "PROMPT",
            "trace_dataset_creator": {
              "project_reader": {
                "object_type": "STRUCTURE",
                "project_path": "[DATA_PATH]/[CURRENT_PROJECT]"
              }
            }
          }
        }
      ]
    }
  ]
}
