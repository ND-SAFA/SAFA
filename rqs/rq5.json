{
  "steps": [
    {
      "jobs": [
        {
          "object_type": "TRAIN",
          "job_args": {
            "random_seed": {
              "*": [
                197,
                914,
                209,
                330
              ]
            }
          },
          "model_manager": {
            "model_path": "roberta-base"
          },
          "trainer_args": {
            "output_dir": "[OUTPUT_PATH]/rq5/models",
            "num_train_epochs": 10,
            "learning_rate": 5e-6,
            "metrics": [
              "map",
              "map_at_k",
              "f",
              "precision"
            ],
            "skip_save": true,
            "use_balanced_batches": {
              "*": [
                true,
                false
              ]
            }
          },
          "trainer_dataset_manager": {
            "train_dataset_creator": {
              "object_type": "TRACE",
              "filter_unlinked_artifacts": {
                "*": [
                  true,
                  false
                ]
              },
              "project_reader": {
                "object_type": "STRUCTURE",
                "project_path": "[DATA_PATH]/cm1"
              }
            },
            "val_dataset_creator": {
              "object_type": "SPLIT",
              "split_strategy": {
                "*": [
                  "SPLIT_BY_SOURCE",
                  "SPLIT_BY_LINK"
                ]
              },
              "val_percentage": 0.25
            },
            "eval_dataset_creator": {
              "object_type": "SPLIT",
              "split_strategy": "SPLIT_BY_LINK",
              "val_percentage": 0.25
            },
            "augmenter": {
              "steps": [
                {
                  "object_type": "RESAMPLE"
                }
              ]
            }
          }
        }
      ]
    }
  ],
  "output_dir": "[OUTPUT_PATH]/rq5/jobs"
}
