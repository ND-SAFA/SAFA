{
  "experiment_type": "ENSEMBLE",
  "steps": [
    {
      "jobs": [
        {
          "object_type": "TRAIN",
          "job_args": {
            "random_seed": 812
          },
          "model_manager": {
            "model_path": {
              "*": [
                "prajjwal1/bert-tiny",
                "thearod5/nl-bert"
              ]
            }
          },
          "trainer_args": {
            "num_train_epochs": 1,
            "per_device_eval_batch_size": 16,
            "per_device_train_batch_size": 4,
            "gradient_accumulation_steps": 2,
            "learning_rate": 5e-5,
            "metrics": [
              "map",
              "map_at_k",
              "f",
              "precision"
            ],
            "skip_save": false,
            "load_best_model_at_end": true
          },
          "trainer_dataset_manager": {
            "train_dataset_creator": {
              "object_type": "TRACE",
              "project_reader": {
                "object_type": "HUB",
                "name": "CM1"
              }
            },
            "val_dataset_creator": {
              "object_type": "SPLIT",
              "val_percentage": 0.10
            },
            "eval_dataset_creator": {
              "object_type": "SPLIT",
              "val_percentage": 0.70
            },
            "augmenter": {
              "steps": [
                {
                  "object_type": "RESAMPLE"
                }
              ]
            }
          }
        }
      ]
    }
  ]
}
