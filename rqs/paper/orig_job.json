{
  "output_dir": "~/desktop/safa/experiments/paper/results/cm1/original",
  "steps": [
    {
      "jobs": [
        {
          "object_type": "PAPER",
          "task": "PREDICT",
          "llm_manager": {
            "object_type": "ANTHROPIC",
            "llm_args": {
              "model": "claude-instant-v1-100k",
              "metrics": [
                "classification",
                "confusion_matrix",
                "specificity",
                "accuracy"
              ]
            }
          },
          "trainer_dataset_manager": {
            "eval_dataset_creator": {
              "object_type": "TRACE",
              "project_reader": {
                "object_type": "STRUCTURE",
                "project_path": "~/desktop/safa/datasets/paper/cm1"
              },
              "data_cleaner": {
                "steps": [
                  {
                    "object_type": "REPLACE_WORDS",
                    "word_replace_mappings": {
                      "_VehicleCore_": "VehicleCore"
                    }
                  }
                ]
              }
            }
          }
        }
      ]
    }
  ]
}
