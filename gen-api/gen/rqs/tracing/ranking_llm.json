{
  "steps": [
    {
      "jobs": [
        {
          "object_type": "RANKING",
          "ranking_pipeline": "LLM",
          "interactive_mode": false,
          "sorter": "[SORTER]",
          "job_args": {
            "export_dir": "[OUTPUT_PATH]/ranking-llm/[CURRENT_PROJECT]",
            "dataset_creator": {
              "object_type": "PROMPT",
              "trace_dataset_creator": {
                "project_reader": {
                  "object_type": "STRUCTURE",
                  "project_path": {
                    "*": {"+": ["[DATA_PATH]", "[PROJECTS_LIST]"]}
                  }
                }
              }
            }
          }
        }
      ]
    }
  ]
}
