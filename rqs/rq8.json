{
  "steps": [
    {
      "jobs": [
        {
          "object_type": "TRAIN",
          "job_args": {
            "random_seed": {
              "*": [
                197,
                914,
                209,
                330
              ]
            }
          },
          "model_manager": {
            "model_path": "sentence-transformers/all-MiniLM-L6-v2"
          },
          "trainer_args": {
            "output_dir": "[OUTPUT_PATH]/rq8/models",
            "train_epochs_range": [
              1,
              21,
              1
            ],
            "per_device_train_batch_size": 8,
            "metrics": [
              "map",
              "map_at_k",
              "f",
              "precision"
            ]
          },
          "trainer_dataset_manager": {
            "train_dataset_creator": {
              "object_type": "SAFA",
              "project_path": "[DATA_PATH]/cchit"
            },
            "val_dataset_creator": {
              "object_type": "SPLIT",
              "val_percentage": 0.2,
              "split_strategy": "source-random"
            },
            "augmenter": {
              "steps": [
                {
                  "object_type": "RESAMPLE"
                }
              ]
            }
          }
        }
      ]
    }
  ],
  "output_dir": "[OUTPUT_PATH]/rq8/jobs"
}
