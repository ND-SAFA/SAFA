{
  "steps": [
    {
      "jobs": [
        {
          "object_type": "TRAIN",
          "job_args": {
            "random_seed": {
              "*": [
                197,
                914,
                209,
                330
              ]
            }
          },
          "model_manager": {
            "model_path": {
              "*": [
                "roberta-base"
              ]
            }
          },
          "trainer_args": {
            "output_dir": "[OUTPUT_PATH]/lhp/rq9/models",
            "train_epochs_range": [
              1,
              21,
              1
            ],
            "per_device_train_batch_size": 8,
            "per_device_eval_batch_size": 4,
            "metrics": [
              "map",
              "map_at_k",
              "f",
              "precision"
            ]
          },
          "trainer_dataset_manager": {
            "train_dataset_creator": {
              "object_type": "STRUCTURE",
              "project_path": {
                "*": [
                  "[DATA_PATH]/cchit",
                  "[DATA_PATH]/drone",
                  "[DATA_PATH]/cm1"
                ]
              }
            },
            "val_dataset_creator": {
              "object_type": "SPLIT",
              "val_percentage": {
                "*": [
                  0.2,
                  0.5,
                  0.8
                ]
              }
            },
            "augmenter": {
              "steps": [
                {
                  "object_type": "RESAMPLE",
                  "percent_to_weight": 1
                }
              ]
            }
          }
        }
      ]
    }
  ],
  "output_dir": "[OUTPUT_PATH]/rq9/jobs"
}
