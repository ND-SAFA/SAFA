{
  "steps": [
    {
      "jobs": [
        {
          "object_type": "TRAIN",
          "job_args": {
            "random_seed": 914
          },
          "model_manager": {
            "model_path": "roberta-base",
            "layers_to_freeze": [
              0,
              1,
              2,
              3,
              4
            ]
          },
          "trainer_args": {
            "output_dir": "[OUTPUT_PATH]/fix/models",
            "train_epochs_range": [
              1,
              21,
              1
            ],
            "per_device_train_batch_size": 4,
            "per_device_eval_batch_size": 4,
            "metrics": [
              "map",
              "f"
            ]
          },
          "trainer_dataset_manager": {
            "train_dataset_creator": {
              "object_type": "SAFA",
              "project_path": "[DATA_PATH]/lhp/vehiclemodes"
            },
            "val_dataset_creator": {
              "object_type": "SPLIT",
              "val_percentage": 0.8
            },
            "augmenter": {
              "steps": [
                {
                  "object_type": "RESAMPLE"
                }
              ]
            }
          }
        }
      ]
    }
  ],
  "output_dir": "[OUTPUT_PATH]/fix/jobs"
}
