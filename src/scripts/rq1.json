{
  "steps": [
    {
      "jobs": [
        {
          "object_type": "TRAIN",
          "job_args": {
            "random_seed": {
              "*": [
                165,
                242
              ]
            }
          },
          "model_manager": {
            "model_path": {
              "*": [
                "bert-base-uncased"
              ]
            }
          },
          "trainer_args": {
            "output_dir": "~/tgen/test/output",
            "num_train_epochs": {
              "*": [
                10
              ]
            },
            "metrics": [
              "map"
            ],
            "_n_gpu": 1
          },
          "trainer_dataset_manager": {
            "train_dataset_creator": {
              "object_type": "STRUCTURE",
              "project_path": "~/tgen/src/testres/data/structure"
            },
            "val_dataset_creator": {
              "object_type": "SPLIT",
              "val_percentage": 0.2
            }
          }
        }
      ]
    }
  ],
  "output_dir": "~/tgen/test/output"
}