{
  "steps": [
    {
      "jobs": [
        {
          "object_type": "TRAIN",
          "job_args": {
            "random_seed": {
              "*": [
                68,
                120
              ]
            }
          },
          "model_manager": {
            "model_path": {
              "*": [
                "bert-base-uncased",
                "prajjwal1/bert-tiny",
                "prajjwal1/bert-mini",
                "prajjwal1/bert-small",
                "prajjwal1/bert-medium"
              ]
            }
          },
          "trainer_args": {
            "output_dir": "[OUTPUT_PATH]/test/models",
            "num_train_epochs": 1,
            "gradient_accumulation_steps": 8,
            "learning_rate": 5e-6,
            "metrics": [
              "map",
              "map_at_k",
              "f",
              "precision"
            ],
            "skip_save": true,
            "load_best_model_at_end": false
          },
          "trainer_dataset_manager": {
            "train_dataset_creator": {
              "object_type": "TRACE",
              "project_reader": {
                "object_type": "STRUCTURE",
                "project_path": "[DATA_PATH]/lhp/heating"
              }
            },
            "val_dataset_creator": {
              "object_type": "SPLIT",
              "split_strategy": "SPLIT_BY_LINK",
              "val_percentage": 0.25
            },
            "eval_dataset_creator": {
              "object_type": "TRACE",
              "project_reader": {
                "object_type": "STRUCTURE",
                "project_path": "[DATA_PATH]/lhp/cooling"
              }
            },
            "augmenter": {
              "steps": [
                {
                  "object_type": "RESAMPLE"
                }
              ]
            }
          }
        }
      ]
    }
  ],
  "output_dir": "[OUTPUT_PATH]/test/jobs"
}
