{
  "steps": [
    {
      "jobs": [
        {
          "object_type": "TRAIN",
          "job_args": {
            "random_seed": {
              "*": [
                165,
                242
              ]
            }
          },
          "model_manager": {
            "model_path": {
              "*": [
                "bert-base-uncased"
              ]
            }
          },
          "trainer_args": {
            "output_dir": "~/projects/safa/tgen/test/output",
            "num_train_epochs": {
              "*": [
                10
              ]
            },
            "per_device_train_batch_size": 4,
            "metrics": [
              "map"
            ],
            "_n_gpu": 1
          },
          "trainer_dataset_manager": {
            "train_dataset_creator": {
              "object_type": "STRUCTURE",
              "project_path": "~/projects/safa/tgen/src/testres/data/structure"
            },
            "val_dataset_creator": {
              "object_type": "SPLIT",
              "val_percentage": 0.8
            },
            "augmenter": {
              "*": [
                {
                  "steps": []
                },
                {
                  "steps": [
                    {
                      "object_type": "SOURCE_TARGET_SWAP"
                    }
                  ]
                }
              ]
            }
          }
        }
      ]
    }
  ],
  "output_dir": "~/projects/safa/tgen/test/output"
}
