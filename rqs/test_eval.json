{
  "steps": [
    {
      "jobs": [
        {
          "object_type": "PREDICT",
          "job_args": {
          },
          "model_manager": {
            "model_path": {
              "*": [
                "thearod5/bert4re2",
                "thearod5/tbert"
              ]
            }
          },
          "trainer_args": {
            "output_dir": "[OUTPUT_PATH]/eval/models",
            "per_device_train_batch_size": 2,
            "metrics": [
              "map",
              "f",
              "precision",
              "recall"
            ]
          },
          "trainer_dataset_manager": {
            "eval_dataset_creator": {
              "object_type": "SAFA",
              "project_path": {
                "*": [
                  "[DATA_PATH]/lhp/heating",
                  "[DATA_PATH]/lhp/cooling",
                  "[DATA_PATH]/lhp/vehiclemodes"
                ]
              }
            }
          }
        }
      ]
    }
  ],
  "output_dir": "[OUTPUT_PATH]/eval/jobs"
}
