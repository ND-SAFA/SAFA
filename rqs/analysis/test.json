{
  "steps": [
    {
      "jobs": [
        {
          "object_type": "TRAIN",
          "job_args": {
            "random_seed": 812
          },
          "model_manager": {
            "model_path": {
              "*": [
                "hf-internal-testing/tiny-random-bert",
                "prajjwal1/bert-medium"
              ]
            }
          },
          "trainer_args": {
            "num_train_epochs": 1,
            "gradient_accumulation_steps": 1,
            "learning_rate": 5e-5,
            "metrics": [
              "map",
              "map_at_k",
              "f",
              "precision"
            ],
            "skip_save": false,
            "use_balanced_batches": true,
            "load_best_model_at_end": true
          },
          "trainer_dataset_manager": {
            "train_dataset_creator": {
              "object_type": "TRACE",
              "project_reader": {
                "object_type": "CSV",
                "project_path": "[OUTPUT_PATH]/analysis-create_dataset/experiment_0/step_0/{\"val_percentage\": 0.7}/train.csv"
              }
            },
            "val_dataset_creator": {
              "object_type": "TRACE",
              "project_reader": {
                "object_type": "CSV",
                "project_path": "[OUTPUT_PATH]/analysis-create_dataset/experiment_0/step_0/{\"val_percentage\": 0.7}/val.csv"
              }
            },
            "eval_dataset_creator": {
              "object_type": "TRACE",
              "project_reader": {
                "object_type": "CSV",
                "project_path": "[OUTPUT_PATH]/analysis-create_dataset/experiment_0/step_0/{\"val_percentage\": 0.7}/eval.csv"
              }
            },
            "augmenter": {
              "steps": [
                {
                  "object_type": "RESAMPLE"
                }
              ]
            }
          }
        }
      ]
    }
  ]
}
