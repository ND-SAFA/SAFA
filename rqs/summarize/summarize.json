{
  "steps": [
    {
      "jobs": [
        {
          "object_type": "SUMMARY",
          "summarize_code_only": true,
          "do_resummarize_artifacts": false,
          "include_subset_by_dir": "app",
          "dataset_creator": {
            "object_type": "PROMPT",
            "trace_dataset_creator": {
              "object_type": "TRACE",
              "project_reader": {
                "object_type": "STRUCTURE",
                "project_path": "[DATA_PATH]/[CURRENT_PROJECT]"
              }
            }
          },
          "export_dir": "[OUTPUT_PATH]/summarize/[CURRENT_PROJECT]"
        }
      ]
    }
  ]
}
